{
  "name": "image-api-project",
  "version": "1.0.0",
  "description": "API endpoint takes as parameters name of image file, width, height to create and serve a resized image",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "build": "npx tsc",
    "jasmine": "jasmine",
    "test": "npm run build && npm run jasmine"
  },
  "keywords": [
    "Node.js",
    "Express",
    "TypeScript",
    "Sharp",
    "image",
    "resize",
    "api"
  ],
  "author": "Tariq Khan",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/got": "^9.6.11",
    "@types/jasmine": "^3.7.7",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.12.2",
    "@types/sharp": "^0.28.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "express": "^4.17.2",
    "express-rate-limit": "^6.2.0",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "multer": "^1.4.4",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.30.0",
    "supertest": "^6.1.6"
  },
  "engines": {
    "node": "^16.x"
  }
}
